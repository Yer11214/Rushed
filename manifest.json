{
  "manifest_version": 3,
  "name": "rushed",
  "version": "1.0",
  "description": "Quick create random value",
  "icons": {
    "16": "assets/16.png",
    "48": "assets/48.png",
    "128": "assets/128.png"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "clipboardWrite"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/16.png",
      "48": "assets/48.png",
      "128": "assets/128.png"
    }
  }
 
}
